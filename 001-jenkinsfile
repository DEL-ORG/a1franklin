pipeline {
    agent any

    stages {
        stage('Checkout SCM') {
            steps {
                // Replace the URL and credentialsId with your actual repository URL and credentials
                git branch: 'main',
                    url: 'git@github.com:DEL-ORG/a1franklin.git',
                    credentialsId: 'new-jenkins-github'
            }
        }

        stage('Hello World') {
            steps {
                echo 'Hello World'
                script {
                    // Use the sh step to run shell commands
                    sh '''
                        echo "Running ls command"
                        ls
                        echo "Current directory:"
                        pwd
                    '''
                }
            }
        }
    }
}
